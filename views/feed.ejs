<div class="container">
<div class="row justify-content-center text-center mt-5">
  <h2>Library Photo Feed</h2>

    <ul class="row list-unstyled">
      <% for(var i=0; i<libraries.length; i++) {%>
        <li class="row-2 justify-content-between mt-5">
          <a href="/library/<%= libraries[i]._id%>">
            <img class="img-fluid img-thumbnail w-25" src="<%= libraries[i].image%>">
          </a>
        </li>
      <% } %>
      </ul>
  </div>
